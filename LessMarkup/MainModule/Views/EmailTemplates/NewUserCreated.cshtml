@model LessMarkup.Framework.Security.Models.NewUserCreatedModel
@{
    Model.Subject = "New User Created";
}
<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title>@Model.Subject</title>
</head>
<body>
    <p>New user was registered in the system:</p>
    <dl>
        <dt>User ID:</dt>
        <dd>@Model.UserId</dd>
        <dt>Name:</dt>
        <dd>@Model.Name</dd>
        <dt>Email:</dt>
        <dd>@Model.Email</dd>
    </dl>
</body>
</html>